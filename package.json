{
    "name": "ember-qunit-testem-launcher",
    "displayName": "ember-qunit-testem-launcher",
    "description": "Run QUnit modules and tests in testem from line in the test file",
    "version": "1.0.0",
    "publisher": "DocX",
    "repository": {
        "type": "git",
        "url": "https://github.com/DocX/ember-qunit-testem-launcher.git"
    },
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openModuleUrl",
        "onCommand:extension.startTestemServer",
        "onCommand:extension.startServerAndOpenModuleUrl"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openModuleUrl",
                "title": "Testem: Run file module"
            },
            {
                "command": "extension.startTestemServer",
                "title": "Testem: Start server in terminal"
            },
            {
                "command": "extension.startServerAndOpenModuleUrl",
                "title": "Testem: Start server and open module"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "sinon": "^7.1.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "@types/superagent": "^3.8.6",
        "superagent": "^4.1.0"
    }
}
